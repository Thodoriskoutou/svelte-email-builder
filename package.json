		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.0.0"
	},
	"dependencies": {
		"@types/react": "^19.0.10",
		"@types/react-dom": "^19.0.4",
		"pocketbase": "^0.25.2",
		"puppeteer": "^24.4.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-email-editor": "^1.7.11",
		"react-router-dom": "^7.3.0"
